Regexp.compile(path);\n            return toPath(params);\n        },\n        handleLink(item) {\n            const { redirect, path } = item;\n            if (redirect) {\n                this.$router.push(redirect);\n                return;\n            }\n            this.$router.push(this.pathCompile(path));\n        },\n    },\n};\n</script>\n\n<style scoped>\n.app-breadcrumb {\n    display: inline-block;\n    font-size: 14px;\n    line-height: 40px;\n    margin-left: 8px;\n}\n\n.no-redirect {\n    color: #97a8be;\n    cursor: text;\n}\n\n.el-breadcrumb__item {\n    height: 40px;\n}\n\n.breadcrumb-enter-active,\n.breadcrumb-leave-active {\n    transition: all 0.5s;\n}\n\n.breadcrumb-enter,\n.breadcrumb-leave-active {\n    opacity: 0;\n    transform: translateX(20px);\n}\n\n.breadcrumb-move {\n    transition: all 0.5s;\n}\n\n.breadcrumb-leave-active {\n    position: absolute;\n}\n</style>\n"],"mappings":";AA0BA;EACAA,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,KAAA;IACAC,OAAAC,KAAA;MACA,IAAAA,KAAA,CAAAC,IAAA,CAAAC,UAAA;QACA;MACA;MACA,KAAAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAD,aAAA;EACA;EACAE,OAAA;IACAF,cAAA;MACA;MACA,IAAAG,OAAA,QAAAP,MAAA,CAAAO,OAAA,CAAAC,MAAA,CACAC,IAAA,IAAAA,IAAA,CAAAC,IAAA,IAAAD,IAAA,CAAAC,IAAA,CAAAC,KAAA,CACA;MACA,MAAAC,KAAA,GAAAL,OAAA;MAEA,UAAAM,WAAA,CAAAD,KAAA;QACAL,OAAA;UAAAL,IAAA;UAAAQ,IAAA;YAAAC,KAAA;UAAA;QAAA,GAAAG,MAAA,CACAP,OAAA,CACA;MACA;MAEA,KAAAT,SAAA,GAAAS,OAAA,CAAAC,MAAA,CACAC,IAAA,IACAA,IAAA,CAAAC,IAAA,IACAD,IAAA,CAAAC,IAAA,CAAAC,KAAA,IACAF,IAAA,CAAAC,IAAA,CAAAK,UAAA,WACA;IACA;IACAF,YAAAZ,KAAA;MACA,MAAAe,IAAA,GAAAf,KAAA,IAAAA,KAAA,CAAAe,IAAA;MACA,KAAAA,IAAA;QACA;MACA;MACA,OACAA,IAAA,CAAAC,IAAA,GAAAC,iBAAA,eAAAA,iBAAA;IAEA;IACAC,YAAAjB,IAAA;MACA;QAAAkB;MAAA,SAAApB,MAAA;MACA,IAAAqB,YAAA,GAAAC,OAAA;MACA,IAAAC,MAAA,GAAAF,YAAA,CAAAG,OAAA,CAAAtB,IAAA;MACA,OAAAqB,MAAA,CAAAH,MAAA;IACA;IACAK,WAAAhB,IAAA;MACA;QAAAiB,QAAA;QAAAxB;MAAA,IAAAO,IAAA;MACA,IAAAiB,QAAA;QACA,KAAAC,OAAA,CAAAC,IAAA,CAAAF,QAAA;QACA;MACA;MACA,KAAAC,OAAA,CAAAC,IAAA,MAAAT,WAA