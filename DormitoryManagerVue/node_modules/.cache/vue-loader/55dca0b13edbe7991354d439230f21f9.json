 -->\n      <el-checkbox @change=\"dateChanged\" v-model=\"check\" :label=\"item.id\">\n        {{ '' }}\n      </el-checkbox>\n      <span class=\"el-checkbox__label\">{{ item.name }}</span> <!-- 显示节点名称 -->\n    </div>\n    <!-- 子节点的展开/收起动画效果 -->\n    <el-collapse-transition>\n      <div v-show=\"childrenDisplay\">\n        <!-- 递归调用tree-node组件，用于展示子节点 -->\n        <tree-node @on-data-change=\"onDateChange\" v-for=\"item1 in item.children\" :checked=\"checked\" :item=\"item1\"\n          :key=\"item1.id\" :padding=\"padding + 18\" class=\"el-tree-node__children\"></tree-node>\n      </div>\n    </el-collapse-transition>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"treeNode\",\n  props: {\n    item: Object,\n    padding: {\n      default: 0\n    },\n    checked: Array\n  },\n  watch: {\n    checked(val) {\n      this.check = val.includes(this.item.id)\n    }\n  },\n  data() {\n    return {\n      check: false,\n      childrenDisplay: false\n    }\n  },\n  methods: {\n    onDateChange(checked, id) {\n      this.$emit('on-data-change', checked, id)\n    },\n    dateChanged() {\n      this.$emit('on-data-change', this.check, this.item.id)\n    }\n  },\n  created() {\n    this.check = this.checked.includes(this.item.id)\n  }\n}\n</script>\n\n<style scoped>\n.el-checkbox__label {\n  padding: 0;\n}\n\n.placeholder {\n  height: 24px;\n  width: 24px;\n}\n</style>\n"]}]}