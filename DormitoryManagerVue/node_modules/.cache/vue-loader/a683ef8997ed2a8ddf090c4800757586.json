   </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {glyphs} from '@/assets/font2/iconfont.json'\n\nexport default {\n  name: 'IconSelect',\n  data() {\n    return {\n      name: '',\n      iconList: glyphs\n    }\n  },\n  methods: {\n    filterIcons() {\n      this.iconList = glyphs\n      if (this.name) {\n        this.iconList = this.iconList.filter(item => item.font_class.includes(this.name))\n      }\n    },\n    selectedIcon(name) {\n      this.$emit('selected', 'icon-' + name)\n      document.body.click()\n    }\n  },\n  created() {\n\n  }\n}\n</script>\n\n<style scoped>\n.icon-body {\n  padding: 10px;\n}\n\n.icon-list {\n  height: 200px;\n  overflow-y: scroll;\n}\n\n.icon-list div {\n  height: 30px;\n  line-height: 30px;\n  margin-bottom: -5px;\n  cursor: pointer;\n  width: 33%;\n  float: left;\n}\n\n.icon-list span {\n  display: inline-block;\n  vertical-align: -0.15em;\n  fill: currentColor;\n  overflow: hidden;\n}\n</style>\n"]}]}